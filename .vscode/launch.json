{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${fileDirname}",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/bin/httpyac.js",
      "args": [
        "send",
        "${file}"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "${defaultBuildTask}"
    }
  ]
}