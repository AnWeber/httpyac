{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${fileDirname}",
      "program": "${workspaceFolder}/bin/httpyac.js",
      "args": ["${fileBasename}", "--all"],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "preLaunchTask": "${defaultBuildTask}"
    }
  ]
}